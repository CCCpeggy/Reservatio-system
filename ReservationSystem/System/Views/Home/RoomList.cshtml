@model List<RoomSystem.Models.RoomModle>

@{
    ViewBag.Title = "RoomsList";
}

<h2>RoomsList</h2>

<p>
    @Html.ActionLink("Create New", "RoomCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().MinNumberOfUsers)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().MaxNumberOfUsers)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MinNumberOfUsers)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxNumberOfUsers)
        </td>
        <td>
            @Html.ActionLink("Edit", "RoomEdit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "RoomDelete", new { id=item.Id })
        </td>
    </tr>
}

</table>
